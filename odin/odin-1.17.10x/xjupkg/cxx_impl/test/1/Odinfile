%a.hh.cxx.sm == a.hh+inc_sp=(..)+impl=(a.cc):impl.cxx.sm
%b.hh.cxx.sm == b.hh+inc_sp=(..)+impl=(b.cc):impl.cxx.sm
%c.hh.cxx.sm == c.hh+inc_sp=(..):impl.cxx.sm
%d.hh.cxx.sm == d.hh+inc_sp=(..)+impl=(d.cc):impl.cxx.sm
%f.hh.cxx.sm == f.hh+inc_sp=(..)+impl=(f.cc):impl.cxx.sm

%a.cc.cxx.sm == %none.hh+impl=(a.cc)+inc_sp=(..):impl.cxx.sm

%none.hh == <<


%a == %a.cc.cxx.sm+inc_sp=(..):exe
